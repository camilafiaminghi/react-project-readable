  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "identity-obj-proxy": "^3.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./src/setupTests.js"
    ]
  }
